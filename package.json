    "@babel/node": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "@babel/preset-typescript": "^7.0.0-beta.44",
    "@types/express": "^4.11.1",
    "@types/morgan": "^1.7.35",
    "@types/node": "^9.6.2",
    "@types/react": "^16.3.5",
    "@types/react-dom": "^16.0.4",
    "@types/webpack": "^4.1.3",
    "babel-loader": "^8.0.0-beta.2",
    "nodemon": "^1.17.3",
    "ts-loader": "^4.2.0",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "tslint-eslint-rules": "^5.1.0",
    "typescript": "^2.8.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14"
  },
  "name": "react-typescript-template",
  "description": "A TypeScript+React+ExpressJS project template for NodeJS",
  "homepage": "https://github.com/cdhgee/react-typescript-template#readme",
  "license": "GPL-3.0",
  "main": "src/server/app.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cdhgee/react-typescript-template.git"
  },
  "scripts": {
    "bundle": "webpack",
    "run:dev": "babel-node --extensions .ts,.tsx src/app.ts --inspect",
    "watch:client": "webpack --watch",
    "watch:server": "nodemon -w src -e .ts,.tsx --exec npm run run:dev"
  },
  "version": "1.0.0"
}
